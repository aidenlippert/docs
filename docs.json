{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Nooterra Protocol",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Docs",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "index",
              "quickstart"
            ]
          },
          {
            "group": "Workflows & Console",
            "pages": [
              "workflows",
              "console",
              "ledger"
            ]
          },
          {
            "group": "Agents",
            "pages": [
              "ai-tools/index",
              "ai-tools/agent-quickstart",
              "ai-tools/agent-starter-template",
              "ai-tools/agent-integration",
              "ai-tools/wrap-agent-30-minutes",
              "sdk-api"
            ]
          },
          {
            "group": "Protocol",
            "pages": [
              "whitepaper",
              "ai-tools/technical-spec",
              "demo-logistics",
              "security",
              "faq",
              "troubleshooting"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "GitHub",
          "href": "https://github.com/nooterra",
          "icon": "github"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
